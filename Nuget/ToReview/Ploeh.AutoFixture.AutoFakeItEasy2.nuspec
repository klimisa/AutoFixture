<?xml version="1.0" encoding="utf-8"?>
<package>
    <metadata>
        <id>AutoFixture.AutoFakeItEasy2</id>
        <version>@build.number@</version>
        <title>AutoFixture with Auto Mocking using FakeItEasy 2.x</title>
        <authors>Christopher Loggins</authors>
        <owners>Christopher Loggins</owners>
        <dependencies>
            <dependency id="AutoFixture" version="@build.number@" />
            <dependency id="FakeItEasy" version="2.0" />
        </dependencies>
        <summary>Turns AutoFixture into an Auto-Mocking Container based on FakeItEasy 2.x.</summary>
        <description>This extension turns AutoFixture into an Auto-Mocking Container. The mock instances are created by FakeItEasy 2.x. To use it, add the AutoFakeItEasyCustomization to your Fixture instance.</description>
        <iconUrl>https://raw.githubusercontent.com/AutoFixture/AutoFixture/master/AutoFixtureLogo200x200.png</iconUrl>
        <language>en-US</language>
        <licenseUrl>https://github.com/AutoFixture/AutoFixture/blob/master/LICENCE.txt</licenseUrl>
        <projectUrl>https://github.com/AutoFixture/AutoFixture</projectUrl>
    </metadata>
    <files>
        <file src="Ploeh.AutoFixture.AutoFakeItEasy2.dll" target="lib\net40" />
        <file src="Ploeh.AutoFixture.AutoFakeItEasy2.pdb" target="lib\net40" />
        <file src="Ploeh.AutoFixture.AutoFakeItEasy2.XML" target="lib\net40" />
        <file src="..\Src\AutoFakeItEasy2\**\*.cs" target="src" />
        <file src="Install.ps1" target="tools" />
    </files>
</package>
